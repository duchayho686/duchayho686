instakillmain.Name = "instakillmain"
instakillmain.Parent = main
instakillmain.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
instakillmain.Position = UDim2.new(0.043478258, 0, 0.720183432, 0)
